{
  "expo": {
    "name": "Project-BA",
    "slug": "Project-BA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_G2_degradé-removebg-preview.png",
    "scheme": "projectba",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tonnom.projectba"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo_G2_degradé-removebg-preview.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tonnom.projectba"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo_G2_degradé-removebg-preview.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_G2_degradé-removebg-preview.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "GreenUp a besoin d'accéder à votre caméra pour vous permettre de prendre en photo vos défis écologiques."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "GreenUp a besoin d'accéder à vos photos pour vous permettre de sélectionner une preuve de défi depuis votre galerie."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}